    "version": "0.2.0",
    "configurations": [
        {
            "name": "FastAPI: Remote Attach",
            "type": "debugpy",
            "request": "attach",
            "connect": {
                "host": "127.0.0.1",
                "port": 5678
            },
            "justMyCode": false,
            "pathMappings": [
                {
                    "localRoot": "${workspaceFolder}",
                    "remoteRoot": "."
                }
            ]
        },
        {
            "name": "Typer: Generate mentorships transcriptions",
            "type": "debugpy",
            "request": "launch",
            "module": "src.app.commands.transcribe_mentorships",
            // "args": [
            //     "transcribe",
            // ],
            "justMyCode": false,
            "pathMappings": [
                {
                    "localRoot": "${workspaceFolder}",
                    "remoteRoot": "."
                }
            ],
        },
        {
            "name": "Typer: Generate mentorships summaries",
            "type": "debugpy",
            "request": "launch",
            "module": "src.app.commands.summarize_mentorships",
            "justMyCode": false,
            "pathMappings": [
                {
                    "localRoot": "${workspaceFolder}",
                    "remoteRoot": "."
                }
            ],
        },
        {
            "name": "Typer: Generate mentorships other details",
            "type": "debugpy",
            "request": "launch",
            "module": "src.app.commands.other_info_mentorships",
            "justMyCode": false,
            "pathMappings": [
                {
                    "localRoot": "${workspaceFolder}",
                    "remoteRoot": "."
                }
            ],
        },
    ]
}